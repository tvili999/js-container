{
    "name": "@tvili999/js-container",
    "version": "3.1.3",
    "license": "MIT",
    "files": [
        "lib",
        "esm",
        "package.json"
    ],
    "exports": {
        ".": {
            "types": "./lib/index.d.ts",
            "require": "./lib/index.js",
            "import": "./esm/index.js"
        },
        "./stages": {
            "types": "./lib/stages/index.d.ts",
            "require": "./lib/stages/index.js",
            "import": "./esm/stages/index.js"
        },
        "./decorators": {
            "types": "./lib/decorators/index.d.ts",
            "require": "./lib/decorators/index.js",
            "import": "./esm/decorators/index.js"
        }
    },
    "scripts": {
        "build": "rimraf lib/* && tsc -p tsconfig.build.json",
        "build:esm": "rimraf esm/* && tsc -p tsconfig.esm.json",
        "test": "jest",
        "test:watch": "jest --watch"
    },
    "dependencies": {
        "@types/node": "^20.4.2",
        "typescript": "^5.1.6"
    },
    "devDependencies": {
        "@types/jest": "^29.5.3",
        "@types/sinon": "^10.0.15",
        "jest": "^29.6.1",
        "rimraf": "^5.0.1",
        "sinon": "^15.2.0",
        "ts-jest": "^29.1.1"
    }
}
